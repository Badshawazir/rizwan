def hasil_crack():
	print("")
	jalan("╰-> 1.Lihat Hasil Crack OK\n╰-> 2.Lihat Hasil Crack CP\n╰-> 3.Kembali")
	inhasil = input(f"╰--> Pilih : ")
	if inhasil in ["1","01"]:
		try:c_o_k = os.listdir('OK')
		except FileNotFoundError:
			jalan("Tidak Ada Hasil")
			time.sleep(2)
			hasil_crack()
		if len(c_o_k)==0:
			jalan("Tidak Ada Hasil")
			time.sleep(2)
			hasil_crack()
		else:
			jalan("Hasil OK")
			cuih = 0
			lol = {}
			for kaoo in c_o_k:
				try:hikmat = open('OK/'+kaoo,'r').readlines()
				except:continue
				cuih+=1
				if cuih<10:
					__oo = '0'+str(cuih)
					lol.update({str(cuih):str(kaoo)})
					lol.update({__oo:str(kaoo)})
					jalan(f"{__oo} {kaoo} {str(len(hikmat))} akun")
				else:
					lol.update({str(cuih):str(kaoo)})
					jalan(f"{cuih} {kaoo} {str(len(hikmat))} akun")
					#kangrecodekontol
			print("Pilih Hasil Untuk Ditmpilkan")
			geeh = input("╰--> pilih : "+H)
			try:geh = lol[geeh]
			except KeyError:
				print("Pilihan Tidak Ada")
				exit()
			try:lin = open('OK/'+geh,'r').read()
			except:
				jalan("File Tidak Ditemukan") 
				time.sleep(2)
				hasil_crack()
			jalan("List Akun OK Kamu\n") 
			#kangrecodekontol
			hus = os.system('cd OK && cat '+geh)
			jalan("List Akun OK Kamu") 
			input("Kembali")
			menu()
	elif inhasil in ["2","02"]:
		try:c_o_k = os.listdir('CP')
		except FileNotFoundError:
			jalan("Tidak Ada Hasil")
			time.sleep(2)
			hasil_crack()
