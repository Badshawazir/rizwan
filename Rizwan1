		metode.append("mobile")
	print("")
	pw_tambahani = input(f"Ingin Menambahkan Password ({M}y{P}/{H}t{P}) : ")
	if pw_tambahani in ["y","Y","yes","Ya","Yes"]:
		pw_tambahan.append("ya")
		jalan(f"Contoh Password : {H}anjing,kontol,sayang")
