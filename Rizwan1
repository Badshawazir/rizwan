	elif nanya_proxy in ["t","T","tidak","Tidak"]:
		proxp()
	else:
		proxp()
	uar = input(f"Ingin Menambahkan User Agent ({M}y{P}/{H}t{P}) : ")
	if uar in ["y","Y","ya"]:
		ua = input(f"Masukan User Agent : ")
		ugent = ua.split(',')
		ugent = open('useragent.txt','w')
		ugent.write(ua)
		ugent.close()
	elif uar in ["t","T","tidak"]:
		pass
	else:
		jalan(f"Isi Yang Benar ")
		crack_public_pilihan()
	print("")
	nanya_apk = input(f"Ingin Menampilkan Aplikasi Yang Terkait ({M}y{P}/{H}t{P}) : ")
	if nanya_apk in ["y","ya","Y"]:
		tampilkan_apk.append("ya")
	elif nanya_apk in ["t","T","tidak"]:
		tampilkan_apk.append("no")
	else:
		tampilkan_apk.append("no")
	pw_tambahai = input(f"Ingin Menambahkan Password Di Nama Belakang ({M}y{P}/{H}t{P}) : ")
	if pw_tambahai in ["y","Y","yes","Ya","Yes"]:
		pw_belakang.append("ya")
		jalan(f"Contoh Password : {H}bekasi,jakarta, bandung")
		pw_nya_cok = input("Password Tambahan : ")
		pw_gw=pw_nya_cok.split(',')
		for cpw in pw_gw:
			pw_lu.append(cpw)
	elif pw_tambahai in ["t","T","Tidak","tidak","no"]:
		pw_belakang.append("no")
	else:
		pw_belakang.append("no")
	pilpas = input(f"Ingin Menggunakan Password Manual ({M}y{P}/{H}t{P}) : ")
	if pilpas in ["y","Y","ya","yes"]:
		with tread(max_workers=30) as HikmatXF:
			jalan(f"Contoh Password : {H}anjing,kontol,ngentot,sayang")
