					#print(P+' •'+H+__oo+P+'• '+kaoo+' • '+str(len(hikmat))+' akun'+P)
				else:
					lol.update({str(cuih):str(kaoo)})
					print(f"{cuih} {kaoo} {str(len(hikmat))} akun")
					#print(P+' •'+H+str(cuih)+P+'• '+kaoo+' • '+str(len(hikmat))+' akun'+P)
			print("Pilih Hasil Untuk Ditampilkan")
			geeh = input("╰--> pilih : "+H)
			try:geh = lol[geeh]
			except KeyError:
				print("Pilihan Tidak Ada")
				exit()
			try:lin = open('CP/'+geh,'r').read()
			except:
				jalan("File Tidak Ditemukan") 
				time.sleep(2)
				hasil_crack()
			jalan("List Akun CP Kamu\n") 
			#x=f"{P2}cd CP*-->{geh}"
			#vprint(panel(x,style=f"{warna_warni_rich_cerah}"))
			hus = os.system('cd CP && cat '+geh)
			jalan("\n"+"List Akun CP Kamu") 
			input(garis+" kembali")
			menu()
	elif inhasil in ["0","00"]:
		menu()
	else:
		jalan(f"Isi Yang Benar ")
		hasil_crack()

def cracked_publickey():
	jalan(f"Ketik {H}y{P} Untuk Crack Massal\n{P}Ketik {M}t {P}Untuk Crack Publik")
	print("")
	cuy = input(f"Apakah Kamu Ingin Crack Massal ({M}y{P}/{H}t{P}) : ")
	if cuy in ["y","Y"]:
		massal_cracked_public()
	elif cuy in ["t","T"]:
		cracked_public()
	elif cuy in ["g","G"]:
		cracked_email()
	else:
		jalan(f"{garis} isi yang benar ")
		cracked_publickey() 

def option_sesi():
	try:c_o_k = os.listdir('CP')
	except FileNotFoundError:
		jalan(garis+" tidak ada hasil")
		time.sleep(2)
		hasil_crack()
	if len(c_o_k)==0:
		jalan(garis+" tidak ada hasil")
		time.sleep(2)
		hasil_crack()
	else:
		x=f"{P2} hasil cp"
		vprint(panel(x,style=f"{warna_warni_rich_cerah}"))
		cuih = 0
		lol = {}
		for kaoo in c_o_k:
			try:hikmat = open('CP/'+kaoo,'r').readlines()
			except:continue
			cuih+=1
			if cuih<10:
				__oo = '0'+str(cuih)
				lol.update({str(cuih):str(kaoo)})
				lol.update({__oo:str(kaoo)})
				x=f"{P2}{kaoo} • {str(len(hikmat))} akun{P2}"
				vprint(panel(x,style=f"{warna_warni_rich_cerah}"))
			else:
				lol.update({str(cuih):str(kaoo)})
				x=f"{P2} {kaoo} • {str(len(hikmat))} akun{P2}"
				vprint(panel(x,style=f"{warna_warni_rich_cerah}"))
	x=f"{P2}contoh : CP/{cpz}"
	vprint(panel(x,style=f"{warna_warni_rich_cerah}"))
	files = input(f"{garis} pilih yang mau di cek : {H}")
	try:
		buka_baju = open(files, "r").readlines()
	except IOError:
		exit("\n%s files %s%s%s tidak ada!"%(garis,H,files,P))
	for memek in buka_baju:
		kontol = memek.replace("\n","")
		titid  = kontol.split("|")
		print(f"\n{garis} account : "+K+(kontol.replace(" + ","")))
		try:
			cek_opsi(titid[0].replace(" + ",""), titid[1])
		except requests.exceptions.ConnectionError:
			pass
	input(f"{garis} enter untuk kembali ")
	menu()

